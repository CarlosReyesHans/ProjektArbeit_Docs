%%% using key/value pairs as options to packages and classes
\RequirePackage{kvoptions}

\SetupKeyvalOptions{%
  family=tuhhcolor,
  prefix=@tuhhcolor@
}

% allow for grayscale option
\newif\if@tuhhcolor@gray
\@tuhhcolor@grayfalse
\DeclareVoidOption{gray}{%
  \PassOptionsToPackage{gray}{xcolor}
  \@tuhhcolor@graytrue
}

\DeclareDefaultOption{%
  \PackageWarning{\tuhhcolor@PackageName}{Unknown option '\CurrentOption'}%
}

\ProcessKeyvalOptions*\relax

\RequirePackage{xcolor}

