    colorlinks=true,        % false: boxed links; true: colored links
    linkcolor=tuhh_darkturquoise,         % color of internal links
    linkbordercolor=tuhh_darkturquoise,   %
    citecolor=tuhh_blue,                  % color of links to bibliography
    citebordercolor=tuhh_blue,            %
    filecolor=tuhh_magenta,               % color of file links
    filebordercolor=tuhh_magenta,         %
    urlcolor=tuhh_darkred,                % color of external links
    urlbordercolor=tuhh_darkred           % color of external links
  }%
\fi
